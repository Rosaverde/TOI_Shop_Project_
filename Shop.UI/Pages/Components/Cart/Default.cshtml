@model IEnumerable<Shop.Application.Cart.GetCart.Response>
@{

}

<div>
    <p class="title">Cart</p>
    @foreach (var product in Model)
    {
        <article class="media">
            <figure class="media-left">
                <p class="image is-64x64">
                    <img src="~/images/258097012-1518655510.jpg" />
                </p>
            </figure>
            <div class="media-content">
                <div class="content">
                    <p class="title">@product.Name</p>
                    <div class="level">
                        <p class="level-item">Qty: @product.Qty</p>
                        <p class="level-item">Size: @product.StockId</p>
                        <p class="level-item">Value: @product.Value</p>
                    </div>
                </div>
            </div>
        </article>
    }
</div>